	<parameter name="daerah" class="java.lang.String"/>
	<parameter name="dinas" class="java.lang.String"/>
	<queryString>
		<![CDATA[select to_char(current_date,'yyyy-MM,dd') as tglcetak, ss.sspdtgl terimatgl, s.tahun||'-'||lpad(ss.sspdno::text,5,'0') terimano, ss.jml_bayar as bayar, 0 as amount, get_npwpd(c.id, true) npwpd, s.customer_id, '' as nobukti,
       c.nama customernm, coalesce(s.dasar,0) as dasar, s.masadari, r.kode rekeningkd, r.nama rekeningnm, s.sptno,
       coalesce(s.denda,0) as denda_entry, coalesce(s.tarif,0) as tarif, s.terimatgl as spttgl,

	(date_part('year',ss.sspdtgl)-date_part('year',s.jatuhtempotgl))*12+(date_part('month',ss.sspdtgl)-date_part('month',s.jatuhtempotgl)) as selisih_bln,

	(case when (date_part('year',ss.sspdtgl)-date_part('year',s.jatuhtempotgl))*12+(date_part('month',ss.sspdtgl)-date_part('month',s.jatuhtempotgl))>0 then
	(date_part('year',ss.sspdtgl)-date_part('year',s.jatuhtempotgl))*12+(date_part('month',ss.sspdtgl)-date_part('month',s.jatuhtempotgl))
		else 0
	end)*0.02*coalesce(s.dasar,0)*coalesce(s.tarif,0) as denda

from pad.pad_spt s
    left join pad.pad_customer c on s.customer_id = c.id
    left join pad.pad_customer_usaha cd on s.customer_usaha_id=cd.id
    left join pad.pad_jenis_pajak p on s.pajak_id = p.id
    left join pad.pad_rekening r on r.id = p.rekening_id
		left join public.pad_invoice inv on inv.source_nama = 'pad_spt' and inv.source_id = s.id
		left join pad.pad_sspd ss on ss.invoice_id = inv.id

where ss.sspdtgl>= date ($P{tglawal}) and ss.sspdtgl<= date ($P{tglakhir})
and cd.usaha_id = 7

order by ss.sspdtgl, r.kode]]>
	</queryString>
