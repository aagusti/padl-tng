	<queryString>
		<![CDATA[select
initcap(rj.jalannm)||' / '||jk.kelasnm||' / '||initcap(lp.lokasinm) as lokasi_pasang,
s.r_judul, s.r_panjang, s.r_lebar, s.r_muka, s.r_banyak, s.r_tinggi, s.r_luas,

format_tgl(date('$P!{tglcetak}'),false, false) as tglcetak, k.kohirtgl, k.kohirno, s.type_id, st.typenm, get_npwpd(c.id, true) as npwpd, case when s.r_nama is not null then r_nama else c.customernm end as customernm, initcap(c.alamat) as alamat,
case when s.so ilike 's' then format_tgl(s.masadari,true, true) else to_char(s.masadari,'DD/MM/YY - ')||to_char(s.masasd,'DD/MM/YY') end as masa, s.jatuhtempotgl,
p.pajaknm, s.dasar, s.tarif, s.pajak_terhutang, ss.sspdtgl,
case when ss.id is null then 0 else coalesce(s.pajak_terhutang,0) end as bayar

from pad_kohir k
inner join pad_spt as s on s.id = k.spt_id
inner join pad_customer_usaha cd on cd.id = s.customer_usaha_id
inner join pad_customer c on cd.customer_id=c.id
inner join pad_pajak p on s.pajak_id=p.id
inner join tblrekening r on r.id=p.rekening_id
inner join pad_spt_type st on st.id=s.type_id
inner join pad_sspd ss on ss.spt_id=s.id and ss.is_valid=1 $P!{kondisi_bayar}

left join pad_rek_jalan rj on rj.id=s.r_jalan_id
left join pad_rek_lokasi_pasang lp on lp.id=s.r_lokasi_pasang_id
left join pad_rek_jalan_klas jk on jk.id=s.r_jalanklas_id

where cd.usaha_id=4 $P!{kondisi}
order by k.kohirno, k.kohirtgl]]>
	</queryString>
