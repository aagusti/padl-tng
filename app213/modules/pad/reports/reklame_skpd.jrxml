	<queryString>
		<![CDATA[SELECT s.id, extract(year from k.kohirtgl)::text tahun,
get_skpdno(s.id) kohirno,
get_bayarno(s.id, 'pad_spt') sptno,
get_npwpd(c.id, true) npwpd, c.nama as customernm, c.wpnama, c.alamat, kel.nama kelurahannm, kec.nama kecamatannm,
get_rekening(r.kode) rekeningkd, r.nama rekeningnm, u.nama usahanm, p.nama pajaknm,
get_rekening(rd.kode) dendakd, rd.nama dendanm,
s.r_judul isi_ringkas,

(select array_to_string(array(select media::text
from (
(select m.media
from pad_spt_reklame r inner join pad_reklame_media m on m.id = r.media_id
where r.spt_id = s.id
)

) a), ', ')) as jenis_pajak,


(select array_to_string(array(select kjln::text || '~' || med::text || ': (' || trim(to_char(volume,'999,999,999,990.000')) || ' m2 x Rp. ' || trim(to_char(nsr,'999,999,999,990.00')) || ') x ' || tarif::text || '% ' || (case when tambahan::text = '0' then '' else ' + ' || trim(to_char(tambahan,'999,999,999,990.00')) end) || ' = Rp. ' || trim(to_char(jumlah,'999,999,999,990.00'))
from (
(select psr.spt_id, kj.singkatan kjln, sp.nama sp, prm.singkatan med, psr.panjang * psr.lebar * psr.tinggi * psr.sisi * psr.banyak * psr.lama volume,
	psr.tambahan, psr.nsr,
	((psr.panjang * psr.lebar * psr.tinggi * psr.sisi * psr.banyak * psr.lama * psr.nsr)*spt.tarif) + psr.tambahan jumlah,
	psr.alamat, spt.tarif*100 tarif
from pad_spt_reklame psr
	inner join pad_spt spt on spt.id = psr.spt_id
	inner join pad_reklame_sudut_pandang sp on sp.id = spt.r_sudut_pandang_id
	inner join pad_reklame_media prm on prm.id = psr.media_id
	inner join pad_reklame_kelas_jalan kj on kj.id = psr.kelas_jalan_id
where psr.spt_id = s.id
)

) a), ' <br> ')) as ukuran,



(select array_to_string(array(select kjln::text || '. ' || sp::text
from (
(select distinct kj.singkatan kjln, sp.nama sp
from pad_spt_reklame psr
	inner join pad_spt spt on spt.id = psr.spt_id
	inner join pad_reklame_sudut_pandang sp on sp.id = spt.r_sudut_pandang_id
	inner join pad_reklame_kelas_jalan kj on kj.id = psr.kelas_jalan_id
where psr.spt_id = s.id
group by kj.singkatan, sp.nama
)

) a), ', ')) as kelas,

(select array_to_string(array(select alamat::text
from (
(select distinct psr.alamat
from pad_spt_reklame psr
	inner join pad_spt spt on spt.id = psr.spt_id
where psr.spt_id = s.id
)

) a), ', ')) as tempat,

(case s.r_tarifid::text
	when '1' then 'Tidak Ada Penambahan Tarif'
	when '2' then 'Produk Rokok +25%'
	when '3' then 'Reklame Pendidikan -25%'
	when '4' then 'Kenaikan +25% dan Pengurangan -25%'
	else ''
end)::text ket_tarif_lain,

(case s.r_tarifid::text
	when '1' then '0'
	when '2' then (round(round(s.dasar*s.tarif))*0.25)
	when '3' then '0'
	when '4' then '0'
	else '0'
end)::float lainnya,

lpad(extract(day from k.kohirtgl)::text, 2,'0') kohir_tgl,
case
when extract(dow from k.kohirtgl)=0 then 'Minggu'
when extract(dow from k.kohirtgl)=1 then 'Senin'
when extract(dow from k.kohirtgl)=2 then 'Selasa'
when extract(dow from k.kohirtgl)=3 then 'Rabu'
when extract(dow from k.kohirtgl)=4 then 'Kamis'
when extract(dow from k.kohirtgl)=5 then 'Jumat'
when extract(dow from k.kohirtgl)=6 then 'Sabtu'
else 'Hari ###'
end kohirhari,

case
when extract(month from k.kohirtgl)=1 then 'Januari'
when extract(month from k.kohirtgl)=2 then 'Pebruari'
when extract(month from k.kohirtgl)=3 then 'Maret'
when extract(month from k.kohirtgl)=4 then 'April'
when extract(month from k.kohirtgl)=5 then 'Mei'
when extract(month from k.kohirtgl)=6 then 'Juni'
when extract(month from k.kohirtgl)=7 then 'Juli'
when extract(month from k.kohirtgl)=8 then 'Agustus'
when extract(month from k.kohirtgl)=9 then 'September'
when extract(month from k.kohirtgl)=10 then 'Oktober'
when extract(month from k.kohirtgl)=11 then 'Nopember'
when extract(month from k.kohirtgl)=12 then 'Desember'
end kohirbulan,

extract(year from k.kohirtgl) kohirtahun,


lpad(extract(day from s.terimatgl)::text, 2,'0') ijintgl,
case
when extract(dow from s.terimatgl)=0 then 'Minggu'
when extract(dow from s.terimatgl)=1 then 'Senin'
when extract(dow from s.terimatgl)=2 then 'Selasa'
when extract(dow from s.terimatgl)=3 then 'Rabu'
when extract(dow from s.terimatgl)=4 then 'Kamis'
when extract(dow from s.terimatgl)=5 then 'Jumat'
when extract(dow from s.terimatgl)=6 then 'Sabtu'
else 'Hari ###'
end ijinhari,

case
when extract(month from s.terimatgl)=1 then 'Januari'
when extract(month from s.terimatgl)=2 then 'Pebruari'
when extract(month from s.terimatgl)=3 then 'Maret'
when extract(month from s.terimatgl)=4 then 'April'
when extract(month from s.terimatgl)=5 then 'Mei'
when extract(month from s.terimatgl)=6 then 'Juni'
when extract(month from s.terimatgl)=7 then 'Juli'
when extract(month from s.terimatgl)=8 then 'Agustus'
when extract(month from s.terimatgl)=9 then 'September'
when extract(month from s.terimatgl)=10 then 'Oktober'
when extract(month from s.terimatgl)=11 then 'Nopember'
when extract(month from s.terimatgl)=12 then 'Desember'
end ijinbulan,

extract(year from s.terimatgl) ijintahun,


case
when extract(dow from s.masadari)=0 then 'Minggu'
when extract(dow from s.masadari)=1 then 'Senin'
when extract(dow from s.masadari)=2 then 'Selasa'
when extract(dow from s.masadari)=3 then 'Rabu'
when extract(dow from s.masadari)=4 then 'Kamis'
when extract(dow from s.masadari)=5 then 'Jumat'
when extract(dow from s.masadari)=6 then 'Sabtu'
else 'Hari ###'
end masadrhari,


lpad(extract(day from s.masadari)::text, 2,'0') masadari_tgl,
get_bulan(extract(month from s.masadari)::int, false) masadari_bulan,
extract( year from s.masadari)::int as masadari_tahun,

case
when extract(dow from s.masasd)=0 then 'Minggu'
when extract(dow from s.masasd)=1 then 'Senin'
when extract(dow from s.masasd)=2 then 'Selasa'
when extract(dow from s.masasd)=3 then 'Rabu'
when extract(dow from s.masasd)=4 then 'Kamis'
when extract(dow from s.masasd)=5 then 'Jumat'
when extract(dow from s.masasd)=6 then 'Sabtu'
else 'Hari ###'
end masasdhari,


lpad(extract(day from s.masasd)::text, 2,'0') masasd_tgl,
get_bulan(extract(month from s.masasd)::int, false) masasd_bulan,
extract( year from s.masasd)::int as masasd_tahun,


to_char(s.jatuhtempotgl, 'DD-MM-YYYY') jatuhtempotgl,

lpad(extract(day from s.jatuhtempotgl)::text, 2,'0') jtempo_tgl,
get_bulan(extract(month from s.jatuhtempotgl)::int, false) jtempo_bulan,
extract( year from s.jatuhtempotgl)::int as jtempo_tahun,

to_char(s.masadari, 'DD MMM YYYY')||' s/d '||to_char(s.masasd, 'DD MMM YYYY') masa,
s.dasar,  s.tarif, round(s.dasar*s.tarif) pajak, s.denda+s.bunga denda, s.pajak_terhutang, to_char(k.kohirtgl, 'DD-MM-YYYY') kohirtgl,
terbilang(s.pajak_terhutang)|| ' RUPIAH' terbilang,
(hit_jdendarek(cast(s.masadari as date), cast(s.terimatgl as date))*2) jdenda,


pg1.nip nip_petugas, pg1.nama petugas, pg1.jabatan jabatan_petugas,
pg2.nip nip_pejabat, pg2.nama pejabat, pg2.jabatan jabatan_pejabat



FROM pad_spt s
inner join pad_kohir k on k.spt_id=s.id
inner join pad_customer c on c.id=s.customer_id
inner join pad_customer_usaha cu on cu.id=s.customer_usaha_id
left join pad_kecamatan kec on kec.id=c.kecamatan_id
left join pad_kelurahan kel on kel.id=c.kelurahan_id and kel.kecamatan_id=kec.id
inner join pad_usaha u on u.id=cu.usaha_id
inner join pad_jenis_pajak p on p.id=s.pajak_id
inner join pad_rekening r on r.id=p.rekening_id
inner join pad_rekening rd on rd.id=p.rekdenda_id
inner join pad_pegawai pg1 on pg1.id = k.petugas_id
inner join pad_pegawai pg2 on pg2.id = k.pejabat_id
WHERE s.id=$P!{spt_id}]]>
