	<queryString>
		<![CDATA[SELECT extract(year from k.kohirtgl)::text tahun,
get_kohirno(s.id) kohirno,
get_npwpd(c.id, true) npwpd, c.customernm, c.wpnama, c.alamat, c.wpalamat, kel.kelurahannm, kec.kecamatannm, get_rekening(r.rekeningkd) rekeningkd, r.rekeningnm, u.usahanm, p.pajaknm, s.r_status, jl.jalannm, jk.kelasnm, nsr.nsrnm, sp.sudutnm, s.r_panjang, s.r_lebar, s.r_tinggi,s.r_njop,
case when lower(p.pajaknm) ilike '%bando%' then 150000 else 100000 end tinggi_val,
r_njop, r_nsr, r_lokasi_pasang_val+r_jalanklas_val+r_sudut_pandang_val skor, s.r_panjang*s.r_lebar luas,
r_njop*(s.r_panjang*s.r_lebar) njop_tot, s.r_tinggi*case when lower(p.pajaknm) ilike '%bando%' then 150000 else 100000 end tinggi_tot, r_nsr*(r_lokasi_pasang_val+r_jalanklas_val+r_sudut_pandang_val) nsr_tot, s.kenaikan, s.kompensasi, case when s.kenaikan>0 then 'Produk Rokok (+25%)' when s.kompensasi>0 then 'Reklame Pendidikan (-25%)' else '' end tarif_produk_ket, case when s.kenaikan>0 then s.kenaikan when s.kompensasi>0 then s.kompensasi else 0 end tarif_produk,
r_lokasi_pasang_val, r_jalanklas_val, r_sudut_pandang_val,

(r_njop*(s.r_panjang*s.r_lebar))+(s.r_tinggi*case when lower(p.pajaknm) ilike '%bando%' then 150000 else 100000 end)+
(r_nsr*(r_lokasi_pasang_val+r_jalanklas_val+r_sudut_pandang_val)) jml_tot,
s.r_muka, s.r_banyak,
case when r_bayarid=1 then 'Tahun' when r_bayarid=2 then 'Semester' when r_bayarid=3 then 'Triwulan' when r_bayarid=4 then 'Bulan' when r_bayarid=5 then 'Minggu' else 'Hari' end jangka_waktu, s.r_lama,

s.r_judul, lp.lokasinm lokasi_pasang,
coalesce(ss.bunga,0) bunga,
coalesce(ss.jml_bayar,0) setoran,

to_char(s.masadari, 'DD-MM-YYYY')||' s/d '||to_char(s.masasd, 'DD-MM-YYYY') masa,
s.dasar,  s.tarif, s.pajak_terhutang, to_char(k.kohirtgl, 'DD-MM-YYYY') kohirtgl

FROM pad_spt s
inner join pad_kohir k on k.spt_id=s.id
inner join pad_rek_lokasi_pasang lp on lp.id=s.r_lokasi_pasang_id
inner join pad_rek_jalan jl on jl.id=s.r_jalan_id
inner join pad_rek_jalan_klas jk on jk.id=s.r_jalanklas_id
inner join pad_rek_sudut_pandang sp on sp.id=s.r_sudut_pandang_id
inner join pad_rek_nsr nsr on nsr.id=s.r_nsr_id
inner join pad_customer c on c.id=s.customer_id
inner join pad_customer_usaha cu on cu.id=s.customer_usaha_id
inner join tblkecamatan kec on kec.id=c.kecamatan_id
inner join tblkelurahan kel on kel.id=c.kelurahan_id and kel.kecamatan_id=kec.id
inner join pad_usaha u on u.id=cu.usaha_id
inner join pad_pajak p on p.id=s.pajak_id
inner join tblrekening r on r.id=p.rekening_id
left join pad_sspd ss on ss.spt_id=s.id and ss.is_valid=1

where cu.id=$P!{cuid} and extract(year from s.terimatgl)=$P!{tahun}
order by s.id=$P!{sptid} desc, s.terimatgl
limit 5]]>
	</queryString>
