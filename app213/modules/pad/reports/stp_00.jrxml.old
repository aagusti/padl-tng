	<queryString>
		<![CDATA[SELECT get_sptno(s.id) sptno, get_bulan(extract(month from masadari)::int,false)||extract(year from masadari) masa_pajak,
get_npwpd(c.id, true) npwpd, c.nama as customernm, c.wpnama, c.alamat,u.nama as usahanm, to_char(s.masadari, 'DD-MM-YYYY')||' s/d '||to_char(s.masasd, 'DD-MM-YYYY') masa,
s.dasar,  s.tarif, s.pajak_terhutang,
get_kohirno(s.id) kohirno, get_rekening(r.kode) rekeningkd, r.nama as rekeningnm,
extract(year from stp.stpdtgl)::text tahun, to_char(stp.stpdtgl, 'DD-MM-YYYY') stpdtgl, to_char(stp.jatuhtempotgl, 'DD-MM-YYYY') jatuhtempotgl,
stp.stpdcount::text, stp.bunga, s.pajak_terhutang+stp.bunga as totalstp,
kel.nama as kelurahannm, kec.nama as kecamatannm

FROM pad_spt s
inner join pad_stpd stp on stp.spt_id=s.id
left  join pad_kohir k on k.spt_id=s.id
inner join pad_customer c on c.id=s.customer_id
inner join pad_customer_usaha cu on cu.id=s.customer_usaha_id
inner join pad_kecamatan kec on kec.id=c.kecamatan_id
inner join pad_kelurahan kel on kel.id=c.kelurahan_id and kel.kecamatan_id=kec.id
inner join pad_usaha u on u.id=cu.usaha_id
inner join pad_jenis_pajak p on p.id=s.pajak_id
inner join pad_rekening r on r.id=p.rekening_id
WHERE stp.id=$P!{stp_id}]]>
	</queryString>
