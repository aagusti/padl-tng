	<parameter name="dinas" class="java.lang.String"/>
	<queryString>
		<![CDATA[select date($P{tglcetak}) as tglcetak, get_npwpd(c.id, true) as npwpd,c.customernm,c.alamat, c.telphone,c.kodepos,
cd.reg_date as createdatecd, cd.notes as ket, kec.kecamatankd,kec.kecamatannm,
kel.kelurahankd, kel.kelurahannm, cd.customer_status_id, u.usahanm,
cd.usaha_id, cd.konterid, cd.notes, kel2.kelurahankd as kel2kd, kel2.kelurahannm as kel2nm,
kec2.kecamatankd as kec2kd, kec2.kecamatannm as kec2nm,
c.kukuhtgl, c.kukuhno, cd.reg_date as reg_datecd
,(select count(*) FROM pad_customer_usaha cd2
     INNER JOIN tblkelurahan k2 ON cd2.kelurahan_id=k2.id
     INNER JOIN tblkecamatan k3 ON k2.kecamatan_id=k3.id
     WHERE cd2.customer_id=c.id and usaha_id=cd.usaha_id and k3.id=kec2.id) as jml
from pad_customer c
inner join pad_customer_usaha cd on c.id=cd.customer_id
inner join tblkelurahan kel on c.kelurahan_id=kel.id
inner join tblkecamatan kec on kel.kecamatan_id=kec.id
inner join pad_usaha u on cd.usaha_id=u.id
inner join tblkelurahan kel2 on cd.kelurahan_id=kel2.id
inner join tblkecamatan kec2 on kel2.kecamatan_id=kec2.id

where c.rp='P' AND cd.usaha_id=$P{usahaid} $P!{kondisi}

order by c.npwpd]]>
	</queryString>
