	<parameter name="dinas" class="java.lang.String"/>
	<queryString>
		<![CDATA[select format_tgl(date('$P!{tglcetak}'),false, false) as tglcetak, k.kohirtgl, k.kohirno, s.type_id, st.typenm, get_npwpd(c.id, true) as npwpd, c.customernm, c.alamat,
case when s.so ilike 's' then format_tgl(s.masadari,true, true) else to_char(s.masadari,'DD/MM/YY - ')||to_char(s.masasd,'DD/MM/YY') end as masa, s.jatuhtempotgl,
p.pajaknm, s.dasar, s.tarif, s.pajak_terhutang, ss.sspdtgl, case when ss.id is null then 0 else s.pajak_terhutang end as bayar

from pad_kohir k
inner join pad_spt as s on s.id = k.spt_id
inner join pad_customer_usaha cd on cd.id = s.customer_usaha_id
inner join pad_customer c on cd.customer_id=c.id
inner join pad_pajak p on s.pajak_id=p.id
inner join pad_spt_type st on st.id=s.type_id
left join pad_sspd ss on ss.spt_id=s.id and ss.is_valid=1
where (k.kohirtgl>=date($P{tglawal}) ) and (k.kohirtgl<=date($P{tglakhir}))
$P!{kondisi}
order by k.kohirno, k.kohirtgl]]>
	</queryString>
